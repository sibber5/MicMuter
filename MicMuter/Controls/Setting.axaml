<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MicMuter.Controls.Setting">
    <Grid Margin="0,5,0,0" ColumnDefinitions="Auto,*" Height="32">
        <TextBlock Name="_textBlock" Grid.Column="0" VerticalAlignment="Center" Margin="0,0,10,0" />
        <ContentControl Name="_contentControl" Grid.Column="1" VerticalAlignment="Stretch" HorizontalContentAlignment="Right" VerticalContentAlignment="Stretch" />
    </Grid>
</UserControl>
